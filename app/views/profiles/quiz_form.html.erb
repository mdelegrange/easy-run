<div class="container col-xs-12 col-md-4 col-md-offset-4">
  <h2>Edit <%= @user.first_name %> <%= @user.last_name %></h2>
<%= form_tag user_registration_path, method: :get do %>
      <%= label_tag 'What is your level !' %>
       <%= select_tag :Level , options_for_select(['beginner', 'intermediate', 'advanced']), class: 'form-control-home' %>
        <div class="btn-home">
          <%= submit_tag "Create", :class => "btn btn-primary btn-lg btn-padding" %>
        </div>
      <% end %>
 
</div>
